        <list id="id2526805" list-type="bulleted">
          <item>Bou jou eie man waarvoor die kinders sal lag:</item>
        </list>
        <list id="id1170035927720" list-type="bulleted">
          <item>Teken ‘n kop en lyf.</item>
        </list>
