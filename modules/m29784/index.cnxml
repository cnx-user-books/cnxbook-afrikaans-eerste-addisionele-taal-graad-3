      <para id="id8253193"/>
      <list id="id1171163768691" list-type="bulleted">
        <item>Luister na die woorde.</item>
        <item>Watter klank hoor jy eerste?</item>
        <item>Dit klink anders as in Engels!</item>
      </list>
      <table id="id1171165339164" summary="">
        <tgroup cols="3">
          <colspec colnum="1" colname="c1"/>
          <colspec colnum="2" colname="c2"/>
          <colspec colnum="3" colname="c3"/>
